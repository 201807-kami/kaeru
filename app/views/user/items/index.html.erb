<p>商品一覧ページ</p>
<%= link_to "artists一覧へ", user_artists_path %>

<%= link_to "ジャンル一覧へ", user_genres_path %>

<p><% @items.each do |item| %></p>

<table>
  <tr>
    <td>商品名</td>
    <td>値段</td>
    <td>アーティスト</td>
    <td>ジャンル</td>
  </tr>
  <tr>
    <td><%= item.title %></td>
    <td><%= item.price %></td>
    <td><%= item.artist_id %></td>
    <td><%= item.genre_id %></td>
    <td><%= link_to "詳細", user_item_path(item.id)%></td>
  </tr>
</table>
<% end %>

